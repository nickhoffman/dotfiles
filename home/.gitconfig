[user]
	name = Nick Hoffman
	email = nick@deadorange.com
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[merge]
  tool = vimdiff
[core]
  editor = /usr/bin/vim
[interactive]
  singlekey = true
[push]
  default = current
[pull]
  default = current
  rebase = true
[hub]
  protocol = ssh
[credential]
  helper = osxkeychain
[init]
  templatedir = ~/.git_template
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
